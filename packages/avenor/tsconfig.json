{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "react",
		"declaration": true,
		"pretty": true,
		"newLine": "lf",
		"stripInternal": true,
		"strict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
    "noImplicitAny": false,
		"noUnusedParameters": false,
		"noFallthroughCasesInSwitch": false,
		"noEmitOnError": false,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}
